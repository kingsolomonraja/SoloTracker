{
  "expo": {
    "name": "SoloTracker",
    "slug": "student-checkin-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "scheme": "studentpunch",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.studentpunch.app"  
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.studentpunch.app" 
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/logo.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a188ce2e-ed20-4bd8-8ad6-14d339a8fe7e"
      }
    },
    "owner": "solomon_30"
  }
}
